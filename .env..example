PORT=3000
PUBLIC_BASE_URL=http://localhost:3000
SESSION_BACKEND=docker
SESSION_IMAGE=local/app-web-session:latest
MANAGER_IMAGE=local/app-web-manager:latest
SESSION_NETWORK=app-web-sessions
SESSION_CACHE_VOLUME=app-web-cache
SESSION_SECRET=change-this-before-prod
ADMIN_API_TOKEN=change-this-before-prod
ALLOW_CUSTOM_APPS=false
SECURE_COOKIES=false
DEFAULT_SESSION_TTL=2h
SESSION_READY_TIMEOUT=90s
REAPER_INTERVAL=30s
SESSION_LOG_TAIL=200
MAX_MANAGER_EVENTS=300
MAX_SESSION_EVENTS=120
DEFAULT_STORAGE_MODE=ephemeral
RESUME_SESSIONS=true
K8S_NAMESPACE=app-web
K8S_SESSION_POD_SERVICE_ACCOUNT=app-web-session
K8S_SESSION_IMAGE_PULL_POLICY=IfNotPresent
K8S_SESSION_CACHE_CLAIM=app-web-session-cache
K8S_SESSION_HOME_CLAIM=app-web-session-home
